{
  "appId": "com.soundswitcher.app",
  "productName": "Sound Switcher",
  "directories": {
    "output": "dist-electron"
  },
  "files": [
    "dist/**/*",
    "electron/**/*",
    "package.json"
  ],
  "extraResources": [
    {
      "from": "electron/tools",
      "to": "tools"
    },
    {
      "from": "electron/scripts",
      "to": "scripts"
    }
  ],
  "win": {
    "target": "dir"
  },
  "forceCodeSigning": false,
  "afterSign": null,
  "afterAllArtifactBuild": null
}
